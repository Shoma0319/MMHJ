#include<stdio.h>
#include<stdlib.h>
#include<time.h>

#define N 8
#define NMAX 100000
#define RMAX 100000
#define QMAX 100000

int main(){

  int i, j, flug;
  int R = 0, Q = 0;
  int rel[RMAX][2], que[QMAX][2];

  srand((unsigned int)time(NULL));

  for(i = 0; i < N; i++){
    for(j = 0; j < N; j++){
        
      flug = rand() % 20 + 1;  
      if(flug >= 15 && i < j){
	rel[R][0] = i+1;
	rel[R][1] = j+1;
	R++;
      }
      else continue;     
    }
  }

  for(i = 0; i < N; i++){
    for(j = 0; j < N; j++){
        
      flug = rand() % 20 + 1;  
      if(flug >= 18 && i != j){
	que[Q][0] = i+1;
	que[Q][1] = j+1;
	Q++;
      }
      else continue;     
    }
  }

  printf("%d %d\n",N,R);
  for(i = 0; i < R; i++)printf("%d %d\n",rel[i][0],rel[i][1]);
  printf("%d\n",Q);
  for(j = 0; j < Q; j++)printf("%d %d\n",que[j][0],que[j][1]);

  return 0;
}
